{
  "name": "ngx-numeric-textbox-upd",
  "version": "10.0.0",
  "license": "MIT",
  "description": "An update to the original ngx-numeric-textbox - an Angular numeric textbox component. Updated to bum Angular as well as npm dependencies",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run test-coverage && npm run e2e",
    "prebuild:lib": "rimraf tmp dist/lib",
    "build:lib": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist/lib",
    "prebuild:demo": "rimraf dist/demo",
    "build:demo": "ng build --prod",
    "pub:lib": "npm run build:lib && npm publish dist/lib",
    "pub:demo": "npm run build:demo -- --base-href /ngx-numeric-textbox/ && gh-pages -d dist/demo",
    "pub": "npm run pub:lib && npm run pub:demo"
  },
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "numeric",
    "textbox",
    "ngx-numeric-textbox",
    "angular numeric textbox component"
  ],
  "author": "Leo Vo <leovo2708@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leovo2708/ngx-numeric-textbox.git"
  },
  "bugs": {
    "url": "https://github.com/leovo2708/ngx-numeric-textbox/issues"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "types": "src/index.d.ts",
  "private": false,
  "peerDependencies": {
    "lodash": "^4.0.0",
    "numeral": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/animations": "^10.2.5",
    "@angular/cli": "^10.2.3",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.11.1",
    "bootstrap": "4.0.0-beta.2",
    "codelyzer": "^5.1.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.6.12",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-inline-ng2-template": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash": "^4.17.21",
    "node-sass": "^4.13.0",
    "numeral": "^2.0.6",
    "protractor": "~7.0.0",
    "rimraf": "^2.7.1",
    "rxjs": "^6.6.7",
    "ts-node": "~3.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7",
    "zone.js": "~0.10.2"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
